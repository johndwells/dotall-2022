<table>
  <thead>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>
  </thead>
  <tbody>
  {% for e in craft.users()
    .search(craft.app.request.getParam('q'))
    .all() %}
    <tr>
      <td>{{ e.firstName }}</td>
      <td>{{ e.lastName }}</td>
      <td>{{ e.email }}</td>
    </tr>
  {% else %}
    <tr>
      <td colspan="3">No results.</td>
    </tr>
  {% endfor %}
  </tbody>
</table>
